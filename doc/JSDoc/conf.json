{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "src/modules/core", "src/modules/vas/core", "src/modules/vas" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./doc/JSDoc/html",
        "recurse": true
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "systemName": "Virtual Atom Smasher",
        "copyright": "(C) Ioannis Charalampidis, Citizen Cyberlab, 2014-2015",
        "navType": "inline",
        "linenums": true,
        "theme": "oblivion",
        "default": {
          "outputSourceFiles": true
        }
    }
}